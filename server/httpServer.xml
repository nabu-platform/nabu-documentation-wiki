        <property key="X-Local-Port"/>
        <property key="X-Request-Relative-URI"/>
        <property key="X-Remote-Is-Local"/>
        <property key="X-Remote-Address"/>
        <property key="X-Remote-User"/>
        <property key="X-Request-Received"/>
        <property key="X-Request-URI"/>
        <property key="X-Request-Protocol"/>
        <property key="X-Request-Security"/>
        <property key="X-Authentication-Scheme"/>
        <property key="X-Remote-Host"/>
        <property key="X-Resource-URI"/>
        <property key="X-Request-Type"/>
    </headerMapping>
</httpServer>
